# Korean translations for hardware-report script

# General
TITLE="하드웨어 보고서"
GENERATED="생성일"
SYSTEM_INFO="시스템 정보"
HOSTNAME="호스트명"
OS="운영체제"
KERNEL="커널"
UPTIME="가동 시간"
LAST_BOOT="마지막 부팅"
BOOT_TIME="부팅 시간"

# CPU Information
CPU_INFO="CPU 정보"
CPU_MODEL="CPU 모델"
CPU_CORES="CPU 코어"
CPU_THREADS="CPU 스레드"
CPU_ARCH="CPU 아키텍처"
CPU_FREQ="CPU 주파수"
CPU_USAGE="CPU 사용량"
CPU_TEMP="CPU 온도"
CPU_LOAD="CPU 부하"

# Memory Information
MEMORY_INFO="메모리 정보"
TOTAL_MEMORY="전체 메모리"
USED_MEMORY="사용 중인 메모리"
FREE_MEMORY="여유 메모리"
MEMORY_USAGE="메모리 사용량"
SWAP_INFO="스왑 정보"
TOTAL_SWAP="전체 스왑"
USED_SWAP="사용 중인 스왑"
FREE_SWAP="여유 스왑"
SWAP_USAGE="스왑 사용량"

# Storage Information
STORAGE_INFO="저장소 정보"
DISK_USAGE="디스크 사용량"
MOUNT_POINT="마운트 포인트"
FILESYSTEM="파일 시스템"
TOTAL_SPACE="전체 공간"
USED_SPACE="사용 중인 공간"
FREE_SPACE="여유 공간"
USAGE_PERCENT="사용량"

# GPU Information
GPU_INFO="GPU 정보"
GPU_MODEL="GPU 모델"
GPU_DRIVER="GPU 드라이버"
