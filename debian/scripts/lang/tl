# Tagalog translations for hardware-report script

# General
TITLE="Ulat ng Hardware"
GENERATED="Nabuo noong"
SYSTEM_INFO="Impormasyon ng Sistema"
HOSTNAME="Hostname"
OS="Operating System"
KERNEL="Kernel"
UPTIME="Oras ng Pagpapatakbo"
LAST_BOOT="Huling Boot"
BOOT_TIME="Oras ng Boot"

# CPU Information
CPU_INFO="Impormasyon ng CPU"
CPU_MODEL="Modelo ng CPU"
CPU_CORES="Mga Core ng CPU"
CPU_THREADS="Mga Thread ng CPU"
CPU_ARCH="Arkitektura ng CPU"
