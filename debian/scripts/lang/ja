# Japanese translations for hardware-report script

# General
TITLE="ハードウェアレポート"
GENERATED="生成日時"
SYSTEM_INFO="システム情報"
HOSTNAME="ホスト名"
OS="オペレーティングシステム"
KERNEL="カーネル"
UPTIME="稼働時間"
LAST_BOOT="最終起動"
BOOT_TIME="起動時間"

# CPU Information
CPU_INFO="CPU情報"
CPU_MODEL="CPUモデル"
CPU_CORES="CPUコア数"
CPU_THREADS="CPUスレッド数"
CPU_ARCH="CPUアーキテクチャ"
CPU_FREQ="CPU周波数"
CPU_USAGE="CPU使用率"
CPU_TEMP="CPU温度"
CPU_LOAD="CPU負荷"

# Memory Information
MEMORY_INFO="メモリ情報"
TOTAL_MEMORY="総メモリ"
USED_MEMORY="使用メモリ"
FREE_MEMORY="空きメモリ"
MEMORY_USAGE="メモリ使用率"
SWAP_INFO="スワップ情報"
TOTAL_SWAP="総スワップ"
USED_SWAP="使用スワップ"
FREE_SWAP="空きスワップ"
SWAP_USAGE="スワップ使用率"

# Storage Information
STORAGE_INFO="ストレージ情報"
DISK_USAGE="ディスク使用率"
MOUNT_POINT="マウントポイント"
FILESYSTEM="ファイルシステム"
TOTAL_SPACE="総容量"
USED_SPACE="使用容量"
FREE_SPACE="空き容量"
USAGE_PERCENT="使用率"

# GPU Information
GPU_INFO="GPU情報"
GPU_MODEL="GPUモデル"
GPU_DRIVER="GPUドライバ"
GPU_MEMORY="GPUメモリ"
GPU_TEMP="GPU温度"
GPU_USAGE="GPU使用率"

# Network Information
NETWORK_INFO="ネットワーク情報"
INTERFACE="インターフェース"
IP_ADDRESS="IPアドレス"
MAC_ADDRESS="MACアドレス"
NETWORK_SPEED="ネットワーク速度"
NETWORK_USAGE="ネットワーク使用率"
DNS_SERVERS="DNSサーバー"
ROUTING_TABLE="ルーティングテーブル"
CONNECTIONS="アクティブ接続"

